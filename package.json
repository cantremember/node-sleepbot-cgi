{
  "name": "node-sleepbot-cgi",
  "version": "0.6.0",
  "description": "A Node.js app to replace legacy Perl CGI scripts for sleepbot.com",
  "author": "Daniel M Foley <webmaster@sleepbot.com>",
  "repository": {
    "type": "git",
    "url": "ssh://git@sink.cantremember.com/opt/git-repo/node-sleepbot-cgi.git"
  },
  "license": "WTFPL",
  "homepage": "http://sleepbot.com",
  "email": "webmaster@sleepbot.com",
  "keywords": [
    "sleepbot"
  ],
  "type": "module",
  "main": "./index.mjs",
  "modules.root": "lib",
  "directories": {
    "app": "./app",
    "lib": "./lib"
  },
  "files": [],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "make build",
    "doc": "make doc",
    "gh-pages": "make gh-pages",
    "postinstall": "make post-install",
    "quality": "make quality",
    "start": "make server",
    "test": "NODE_ENV=test  make test"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "app/**/*.mjs",
      "bin/**/*.mjs",
      "index.mjs",
      "lib/**/*.mjs"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ],
    "require": [],
    "extension": [
      ".mjs"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/coverage"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^5.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "glob": "^7.0.5",
    "lodash-es": "^4.17.21",
    "minimist": "^1.2.6",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "nconf": "~0.11.3"
  },
  "devDependencies": {
    "eslint": "^8.17.0",
    "gulp": "^4.0.2",
    "gulp-clean": "~0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-eslint": "^6.0.0",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "jsdoc": "^3.5.5",
    "mitm": "^1.3.3",
    "mocha": "^9.2.2",
    "mock-fs": "^5.1.2",
    "nock": "^9.3.0",
    "node-mocks-http": "^1.7.0",
    "notes": "~0.0.4",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^13.0.2",
    "supertest": "^3.1.0"
  }
}
