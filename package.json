{
  "name": "node-sleepbot-cgi",
  "version": "0.5.0",
  "description": "A Node.js app to replace legacy Perl CGI scripts for sleepbot.com",
  "author": "Daniel M Foley <webmaster@sleepbot.com>",
  "repository": {
    "type": "git",
    "url": "ssh://git@sink.cantremember.com/opt/git-repo/node-sleepbot-cgi.git"
  },
  "license": "WTFPL",
  "homepage": "http://sleepbot.com",
  "email": "webmaster@sleepbot.com",
  "keywords": [
    "sleepbot"
  ],
  "main": "./index.js",
  "module": "./index.mjs",
  "modules.root": "lib",
  "directories": {
    "app": "./app",
    "lib": "./lib"
  },
  "files": [],
  "readmeFilename": "README.md",
  "scripts": {
    "build": "make build",
    "doc": "make doc",
    "gh-pages": "make gh-pages",
    "postinstall": "make post-install",
    "quality": "make quality",
    "start": "make server",
    "test": "NODE_ENV=test  make test"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "esm": {
    "mode": "strict",
    "await": false
  },
  "nyc": {
    "check-coverage": false,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "app/**/*.js",
      "app/**/*.mjs",
      "bin/**/*.js",
      "bin/**/*.mjs",
      "index.js",
      "index.mjs",
      "lib/**/*.js",
      "lib/**/*.mjs"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "json",
      "text-summary"
    ],
    "require": [],
    "extension": [
      ".js",
      ".mjs"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/coverage"
  },
  "dependencies": {
    "axios": "~0.19.2",
    "bluebird": "^3.4.1",
    "cookie-parser": "^1.4.3",
    "csv-parse": "^4.12.0",
    "ejs": "^2.6.1",
    "esm": "^3.0.46",
    "express": "^4.17.3",
    "glob": "^7.0.5",
    "lodash": "^4.17.10",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.10.0",
    "nconf": "~0.10.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "gulp": "^4.0.2",
    "gulp-clean": "~0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-eslint": "^4.0.2",
    "gulp-jsdoc3": "^2.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "jsdoc": "^3.5.5",
    "mitm": "^1.3.3",
    "mocha": "^5.2.0",
    "mock-fs": "^4.12.0",
    "nock": "^9.3.0",
    "node-mocks-http": "^1.7.0",
    "notes": "~0.0.4",
    "nyc": "^15.1.0",
    "proxyquire": "^1.7.10",
    "rewire": "^4.0.1",
    "sinon": "^5.0.10",
    "supertest": "^3.1.0"
  }
}
