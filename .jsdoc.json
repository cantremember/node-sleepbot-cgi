{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "app/",
            "app.js",
            "config/",
            "lib/",
            "index.js",
            "views/",
            "README.md"
        ],
        "exclude": []
    },
    "plugins": [
        "plugins/markdown",
        "plugins/partial",
        "plugins/sourcetag"
    ],
    "markdown": {
        "parser": "gfm",
        "hardwrap": true
    },
    "opts": {
        "recurse": true,
        "template": "templates/default",
        "destination": "./doc",
        "encoding": "utf8",
        "lenient": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": true,
        "default": {
            "includeDate": false,
            "layoutFile": "support/jsdoc-layout.tmpl"
        }
    }
}
