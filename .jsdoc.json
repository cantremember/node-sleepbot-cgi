{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "gulp.mjs",
      "index.mjs",
      "app/",
      "bin/",
      "config/",
      "lib/",
      "views/",
      "README.md"
    ],
    "includePattern": ".+\\.(m|)js(doc|x)?$",
    "exclude": []
  },
  "plugins": [
    "plugins/markdown",
    "plugins/partial",
    "plugins/sourcetag"
  ],
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "recurse": true,
    "template": "templates/default",
    "destination": "./build/doc",
    "encoding": "utf8",
    "lenient": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "default": {
      "includeDate": false,
      "layoutFile": "support/jsdoc-layout.tmpl"
    }
  }
}
